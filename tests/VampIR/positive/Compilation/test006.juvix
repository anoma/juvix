-- higher-order inductive types
module test006;

import Stdlib.Prelude open;
import Stdlib.Data.Nat.Ord open;

type enum :=
  | opt0 : enum
  | opt1 : Bool -> enum
  | opt2 : Bool -> (Bool -> Bool) -> enum
  | opt3 : Bool -> (Bool -> Bool -> Bool) -> Bool -> enum;

natToBool : Nat -> Bool
  | x := if (x == 0) false true;

boolToNat : Bool -> Nat
  | x := if x 1 0;

main : Nat -> Nat -> Nat
  | x y :=
    boolToNat
      $ case opt3
        (natToBool x)
        Î» {x y := if y false x}
        (natToBool y)
        | opt0 := false
        | opt1 b := b
        | opt2 b f := f b
        | opt3 b1 f b2 := f b1 b2;
