module Case;

import Stdlib.Prelude open;

not' : Bool → Bool
  | b :=
    case b
      | true := false
      | false := true;

terminating
andList : List Bool → Bool
  | l :=
    case l
      | nil := true
      | x :: xs := x && andList xs;

main : IO :=
  printBoolLn (not' false)
    >> printBoolLn (andList (true :: false :: nil));
