module HigherOrderLambda;

open import Stdlib.Prelude;

map' : {A : Type} → {B : Type} → (A → B) → List A → List B;
map' f := \{ nil := nil | (h :: t) := f h :: map' f t};

lst : List Nat;
lst := zero :: suc zero :: nil;

main : IO;
main := printNatLn (foldr (+) zero (map' ((+) (suc zero)) lst));

end;
