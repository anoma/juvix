-- letrec 2

def writeLn := \x write x >> write "\n";

def mutrec :=
  let one := 1 in
  let two := 2 in
  let three := 3 in
  letrec[a b c]
    a := \x b one;
    b := \x c two;
    c := \x three
  in writeLn (a 5);

-- bl = [three, two, one]
-- unfiltered free vars of defs = {one$5, two$4, three$3, b$2, c$1}
-- filtered free vars of defs = {one$2, two$1, three$0}


letrec x := 3 in mutrec
