(case-on
    so1     ;left
    so1     ;right
    (coprod      ; codomain
    so1
    so1)

    (right so1 1) ; left + right
    (lamb     ; left -> codomain
        so1
        (coprod
            so1
            so1)
        (right
            so1
            unit))
    (lamb     ; right -> codomain
        so1
        (coprod
            so1
            so1)
        (left
            so1
            unit)))