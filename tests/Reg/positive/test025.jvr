function main() : *;

function f(integer, integer, integer) : integer {
  tmp[0] = arg[0];
  tmp[1] = arg[1];
  tmp[2] = arg[2];
  tmp[1] = add tmp[2] tmp[1];
  tmp[0] = mul tmp[1] tmp[0];
  ret tmp[0];
}

function app(integer → integer, integer) : integer {
  tmp[0] = arg[1];
  tmp[1] = arg[0];
  tmp[0] = tcall tmp[1] (tmp[0]), live: (arg[0], arg[1]);
}

function g(*) : * {
  tmp[0] = 1;
  tmp[1] = 2;
  tmp[2] = arg[0];
  tmp[0] = tccall tmp[2] (tmp[1], tmp[0]), live: (arg[0]);
