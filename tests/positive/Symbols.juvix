module Symbols;

  open import Stdlib.Data.Nat;

  ╘⑽╛ : ℕ;
  ╘⑽╛ ≔ suc nine;

   -- no - function!?
   - : ℕ -> ℕ -> ℕ;
   - ≔ (+);

  （-） : ℕ -> ℕ -> ℕ;
  （-） ≔ (-);

  （*） : ℕ -> ℕ -> ℕ;
  （*） ≔ (*);

  infixl 6 －;
  － : ℕ -> ℕ -> ℕ;
  － ≔ （-）;

  infixl 7 ·;
  · : ℕ -> ℕ -> ℕ;
  · ≔ （*）;

  （0） : ℕ;
  （0） ≔ ╘⑽╛ － ╘⑽╛ · zero;

  主功能 : ℕ;
  主功能 ≔ （0）;
end;