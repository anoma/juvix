module LetSynonym;
  open import Stdlib.Prelude;

  type T :=
    | t : T;

  main : T;
  main :=
    let
      A : Type;
      A := T;
      x : A;
      x := t;
      odd : _;
      even : _;
      odd zero := false;
      odd (suc n) := even n;
      even zero := true;
      even (suc n) := odd n;
    in x;
end;
