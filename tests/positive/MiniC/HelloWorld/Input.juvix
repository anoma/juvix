module Input;

builtin string axiom String : Type;

compile String {
  c ↦ "char*";
};

axiom Action : Type;

compile Action {
  c ↦ "int";
};

axiom put-str-ln : String → Action;

compile put-str-ln {
  ghc ↦ "putStrLn";
  c ↦ "putStrLn";
};

main : Action;
main := put-str-ln "hello world!";

end;
