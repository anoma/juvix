module DefaultValues;

import Stdlib.Data.Product open;

axiom A : Type;

axiom B : Type;

axiom C : Type;

axiom D : Type;

axiom a : A;

axiom b : B;

axiom c : C;

axiom d : D;

mk {f1 : A := a} {f2 : Type} {f3 : C := c} (x : f2)
  : A × f2 × C := f1, x, f3;

x1 : A × B × C := mk (x := b);

mk2 {f1 : A := a} {f2 : Type} {f3 : C := c} (x : f2) {f4 : D := d} (y : f2)
  : A × f2 × C := f1, x, f3;

x2 : A × B × C := mk2 (x := b) (y := b);

mk3 {A := Type} {f1 : D := d} : D := f1;

x3 : D := mk3;

mk4 {A := Type} {f1 f2 f3 : D := d} : _ := f3;

x4 : D := mk4;

mk5 {A := Type} {f1 f2 f3 : D := mk4} : _ := f3;

x5 : D := mk5;

rec1 {a1 : A := rec2 {a}} : A := rec2 {a1 := a1};

rec2 {a1 : A := rec1 {a}} : A := a1;
