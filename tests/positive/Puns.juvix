module Puns;

type A := a;

open A;

type B := b;

open B;

type S :=
  mk@{
    fieldA : A;
    fieldB : B;
    fieldC : A;
    fieldD : B;
    fieldE : B;
  };

f (fieldA : A) (fieldB : B) : S :=
  let
    fieldD := b;
  in S.mk@{
       fieldC := fieldA;
       fieldA;
       fieldB;
       fieldE := b;
       fieldD;
     };
