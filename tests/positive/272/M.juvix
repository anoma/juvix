module M;

type Bool :=
  | true : Bool
  | false : Bool;

type T :=
  | t : T;

type Nat :=
  | zero : Nat
  | suc : Nat → Nat;

f : _
  | false false := true
  | true _ := false;

type Pair (A : Type) (B : Type) :=
  | mkPair : A → B → Pair A B;

g : _
  | (mkPair (mkPair zero false) true) := mkPair false zero;
