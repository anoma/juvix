-- mutual recursion

function g(integer) : integer;

function f(integer) : integer {
  push 1;
  push arg[0];
  lt;
  br {
    true: {
      push 1;
      ret;
    }
    false: {
      push 268435456;
      push 1;
      push arg[0];
      sub;
      call g;
      push arg[0];
      push 2;
      mul;
      add;
      mod;
      ret;
    }
  };
}

function h(integer) : integer;

function g(integer) : integer {
  push 1;
  push arg[0];
