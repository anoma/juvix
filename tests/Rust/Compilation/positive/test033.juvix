-- eta-expansion of builtins and constructors
module test033;

import Stdlib.Prelude open;

f : (Nat → Nat) → Nat
  | g := g 2;

f' : Nat → Nat
  | x := f ((+) x);

g : (Nat → Nat × Nat) → Nat × Nat
  | f := f 2;

g' : Nat → Nat × Nat
  | x := g ((,) x);

f1' : Nat → Nat → Nat
  | x y := f ((+) (div x y));

g1' : Nat → Nat → Nat × Nat
  | x y := g ((,) (div x y));

h : (Nat → Nat → Nat × Nat) → Nat × Nat
  | f := f 1 2;

main : Nat :=
  f' 7
    + fst (g' 7)
    + snd (g' 7)
    + f1' 7 2
    + fst (g1' 7 2)
    + snd (g1' 7 2)
    + fst (h (,))
    + snd (h (,));
