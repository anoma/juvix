-- pattern matching nullary constructor
module test040;

open import Stdlib.System.IO;
open import Stdlib.Data.Bool;

type Unit :=
  | unit : Unit;

type Foo (A : Type) :=
  | foo : Unit -> A -> Foo A;

f : {A : Type} -> Foo A -> A;
f (foo unit a) := a;

main : IO;
main := printBoolLn (f (foo unit true));
