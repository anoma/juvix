-- Do notation
module test084;

type Phantom (M : Type) := nothing;

type T := mkt;

type Monad (f : Type -> Type) :=
  mkMonad@{
  bind : f T -> (T -> f T) -> f T};

open Monad;

maybeMonad : Monad Phantom :=
  mkMonad@{
    bind : Phantom T -> (T -> Phantom T) -> Phantom T
      | _ _ := nothing;
  };

minusThree : Phantom T :=
  bind maybeMonad
    (nothing {T})
    λ{_ :=
      let
        x2' : T := mkt;
      in bind maybeMonad nothing  λ{_ := nothing}};

main : Phantom T := minusThree;
