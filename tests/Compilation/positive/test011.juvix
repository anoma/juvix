-- tail recursion: compute the n-th Fibonacci number in O(n)
module test011;

import Stdlib.Prelude open;

fib' : Nat → Nat → Nat → Nat
  | x y zero := x
  | x y (suc n) := fib' y (x + y) n;

fib : Nat → Nat := fib' 0 1;

main : IO :=
  printNatLn (fib 10)
    >> printNatLn (fib 100)
    >> printNatLn (fib 1000);
