-- tail recursion: compute the n-th Fibonacci number in O(n)
module test011;

open import Stdlib.Prelude;

fib' : Nat → Nat → Nat → Nat;
fib' x y zero := x;
fib' x y (suc n) := fib' y (x + y) n;

fib : Nat → Nat;
fib := fib' 0 1;

main : IO;
main := printNatLn (fib 10) >> printNatLn (fib 100) >> printNatLn (fib 1000);

end;
