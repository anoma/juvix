-- Euclid's algorithm
module test025;

import Stdlib.Prelude open;

terminating
gcd : Nat → Nat → Nat
  | a b :=
    if
      | a > b := gcd b a
      | a == 0 := b
      | else := gcd (mod b a) a;

main : IO :=
  printNatLn (gcd (3 * 7 * 2) (7 * 2 * 11))
    >> printNatLn (gcd (3 * 7 * 2 * 11 * 5) (7 * 2 * 5))
    >> printNatLn (gcd 3 7)
    >> printNatLn (gcd 7 3)
    >> printNatLn (gcd (11 * 7 * 3) (2 * 5 * 13));

