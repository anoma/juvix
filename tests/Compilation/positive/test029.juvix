-- Ackermann function
module test029;

import Stdlib.Prelude open;

ack : Nat → Nat → Nat
  | zero n := n + 1
  | (suc m) zero := ack m 1
  | (suc m) (suc n) := ack m (ack (suc m) n);

main : IO :=
  printNatLn (ack 0 7)
    >> printNatLn (ack 1 7)
    >> printNatLn (ack 1 13)
    >> printNatLn (ack 2 7)
    >> printNatLn (ack 2 13)
    >> printNatLn (ack 3 7);
