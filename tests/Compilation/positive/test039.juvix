-- Mutually recursive let expressions
module test039;

import Stdlib.Prelude open;

main : IO :=
  let
    Ty : Type := Nat;
    odd : _
      | zero := false
      | (suc n) := not (even n);
    unused : _ := 123;
    even : _
      | zero := true
      | (suc n) := not (odd n);
    plusOne (n : Ty) : Ty := n + 1;
  in printBoolLn (odd (plusOne 13))
    >> printBoolLn (even (plusOne 12));
