type optbool :=
  | Just : Bool -> optbool
  | Nothing : optbool;

natToBool (x : Nat) : Bool := if (x == 0) false true;

{-# public: [b] #-}
main (a b : Nat) : Bool :=
  Î» {
      | x (Just b) := if x false b
      | _ Nothing := false
    }
    (natToBool a)
