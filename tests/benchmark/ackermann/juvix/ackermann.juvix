-- Ackermann function (higher-order definition)
module ackermann;

import Stdlib.Prelude open;
import Stdlib.Data.Nat.Ord open;

iter : {A : Type} → (A → A) → Nat → A → A
  | f zero := id
  | f (suc n) := f ∘ iter f n;

step : (Nat → Nat) → Nat → Nat
  | f n := iter f (suc n) 1;

ackermann : Nat → Nat → Nat
  | m := iter step m suc;

main : IO := printNatLn (ackermann 3 11);
