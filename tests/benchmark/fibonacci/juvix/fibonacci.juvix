-- compute the Nth Fibonacci number modulo 2^28
module fibonacci;

import Stdlib.Prelude open;
import Stdlib.Data.Nat.Ord open;

go : Nat → Nat → Nat → Nat
  | zero n _ := n
  | (suc k) n m := go k m (mod (n + m) 268435456);

fib : Nat → Nat
  | n := go n 0 1;

main : IO := printNatLn (fib 100000000);
