-- count combinations of numbers 1 to N having sum N
module combinations;

import Stdlib.Prelude open;
import Stdlib.Data.Nat.Ord open;

terminating
go : Nat → Nat → Nat
  | _ zero := 1
  | zero _ := 0
  | n s :=
    if
      (s < n)
      (go (sub n 1) s)
      (go n (sub s n) + go (sub n 1) s);

combinations : Nat → Nat
  | n := go n n;

main : IO := printNatLn (combinations 100);
